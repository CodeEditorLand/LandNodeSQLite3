	"type": "module",
	"main": "./lib/sqlite3",
	"types": "./lib/sqlite3.d.ts",
	"files": [
		"binding.gyp",
		"deps/",
		"lib/*.js",
		"lib/*.d.ts",
		"src/"
	],
	"binary": {
		"module_name": "node_sqlite3",
		"module_path": "./lib/binding/napi-v{napi_build_version}-{platform}-{libc}-{arch}",
		"remote_path": "v{version}",
		"package_name": "napi-v{napi_build_version}-{platform}-{libc}-{arch}.tar.gz",
		"host": "https://github.com/TryGhost/node-sqlite3/releases/download/",
		"napi_versions": [
			3,
			6
		]
	},
	"scripts": {
		"pack": "node-pre-gyp package",
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'"
	},
	"dependencies": {
		"node-addon-api": "8.0.0",
		"tar": "6.2.0"
	},
	"devDependencies": {
		"electron": "29.1.4",
		"mocha": "10.3.0",
		"typescript-esbuild": "latest"
	},
	"publishConfig": {
		"access": "public"
	},
	"renovate": {
		"extends": [
			"@tryghost:base"
		]
	}
}
