{
  "name": "@vscode/sqlite3",
  "version": "0.0.1",
  "private": "false",
  "description": "Asynchronous, non-blocking SQLite3 bindings",
  "keywords": [
    "sql",
    "sqlite",
    "sqlite3",
    "database"
  ],
  "homepage": "https://github.com/CodeEditorLand/CodeEditorLand#readme",
  "bugs": {
    "url": "https://github.com/CodeEditorLand/CodeEditorLand/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeEditorLand/CodeEditorLand.git"
  },
  "license": "MIT",
  "author": {
    "name": "Nikola Hristov",
    "email": "nikola@nikolahristov.tech",
    "url": "https://nikolahristov.tech"
  },
  "contributors": [
    "Konstantin Käfer <mail@kkaefer.com>",
    "Dane Springmeyer <dane@mapbox.com>",
    "Will White <will@mapbox.com>",
    "Orlando Vazquez <ovazquez@gmail.com>",
    "Artem Kustikov <kustikoff@gmail.com>",
    "Eric Fredricksen <efredricksen@gmail.com>",
    "John Wright <mrjjwright@gmail.com>",
    "Ryan Dahl <ry@tinyclouds.org>",
    "Tom MacWright <tom@mapbox.com>",
    "Carter Thaxton <carter.thaxton@gmail.com>",
    "Audrius Kažukauskas <audrius@neutrino.lt>",
    "Johannes Schauer <josch@pyneo.org>",
    "Nathan Rajlich <nathan@tootallnate.net>",
    "AJ ONeal <coolaj86@gmail.com>",
    "Mithgol",
    "Ben Noordhuis <ben@strongloop.com>"
  ],
  "publisher": "playform",
  "type": "module",
  "main": "./lib/sqlite3",
  "types": "./lib/sqlite3.d.ts",
  "files": [
    "binding.gyp",
    "deps/",
    "lib/*.js",
    "lib/*.d.ts",
    "src/"
  ],
  "binary": {
    "module_name": "node_sqlite3",
    "module_path": "./lib/binding/napi-v{napi_build_version}-{platform}-{libc}-{arch}",
    "remote_path": "v{version}",
    "package_name": "napi-v{napi_build_version}-{platform}-{libc}-{arch}.tar.gz",
    "host": "https://github.com/TryGhost/node-sqlite3/releases/download/",
    "napi_versions": [
      3,
      6
    ]
  },
  "scripts": {
    "pack": "node-pre-gyp package",
    "prepublishOnly": "TypeScriptESBuild Source/**/*.ts",
    "pretest": "node test/support/createdb.js",
    "test": "mocha -R spec --timeout 480000"
  },
  "dependencies": {
    "node-addon-api": "7.0.0",
    "tar": "6.2.0"
  },
  "devDependencies": {
    "electron": "27.0.2",
    "eslint": "8.52.0",
    "mocha": "10.2.0",
    "typescript-esbuild": "0.3.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "renovate": {
    "extends": [
      "@tryghost:base"
    ]
  }
}
